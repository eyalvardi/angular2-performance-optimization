{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "types": [],
    "baseUrl": "./",
    "outDir": "dist/tsc-out"
  },
  "exclude": [
    "node_modules/*",
    "aot/*",
    "**/*-aot.ts"
  ]
}
